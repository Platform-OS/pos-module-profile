{% liquid
  function profile_module = 'modules/core/queries/variable/get', name: 'PROFILE_MODULE', default: 'modules/profile'
  assign function_name = profile_module | append: '/queries/profiles/find'
if slug
 assign user_id =slug[2]
endif
  function profile = function_name, user_id: user_id, id: id, first_name: slug[0] , last_name:slug[1] , uuid: uuid, filters: filters
  return profile
%}
